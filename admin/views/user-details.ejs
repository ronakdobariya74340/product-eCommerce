  <div class="details-wrapper">
    <div class="row">
      <div class="col-12 detail-right">
        <div class="tab-item-group mt-15">
          <div class="nav stream-nav d-flex" id="v-pills-tab" role="tablist" aria-orientation="vertical">
            <button class="nav-link st-nav-link active" id="account-info-tab" data-bs-toggle="pill" data-bs-target="#account-info" type="button" role="tab" aria-controls="account-info" aria-selected="true" tabindex="-1">
              <span>
                Account Info
              </span>
            </button>
            <button class="nav-link st-nav-link" id="celeb-limit-tab" data-bs-toggle="pill" data-bs-target="#celeb-limit" type="button" role="tab" aria-controls="celeb-limit" aria-selected="true" tabindex="-1">
              <span>
                Product Limits
              </span>
            </button>
            <button class="nav-link st-nav-link" id="sb-transaction-listing-tab" data-bs-toggle="pill" data-bs-target="#transaction-list-tab" type="button" role="tab" aria-controls="transaction-list-tab" aria-selected="false" tabindex="-1">
              <span>
                Transactions
              </span>
            </button>

            <button class="nav-link st-nav-link" id="my-limit-tab" data-bs-toggle="pill" data-bs-target="#my-limit" type="button" role="tab" aria-controls="my-limit" aria-selected="true" tabindex="-1">
              <span>
                My Entries
              </span>
            </button>


            <button class="nav-link st-nav-link" id="sb-gift-card-tab" data-bs-toggle="pill" data-bs-target="#gift-card-list-tab" type="button" role="tab" aria-controls="gift-card-list-tab" aria-selected="false" tabindex="-1">
              <span>
                Gift Cards
              </span>
            </button>
            <button class="nav-link st-nav-link" id="sb-redeemed-tab" data-bs-toggle="pill" data-bs-target="#redeemed-list-tab" type="button" role="tab" aria-controls="redeemed-list-tab" aria-selected="false" tabindex="-1">
              <span>Redeemed</span>
            </button>
            <button class="nav-link st-nav-link" id="contes-tab" data-bs-toggle="pill" data-bs-target="#contest" type="button" role="tab" aria-controls="contest-tab"  aria-selected="false" tabindex="-1">
                <span>Contests</span>
            </button>
            <button class="nav-link st-nav-link" id="sb-suspension-history-tab" data-bs-toggle="pill" data-bs-target="#sb-suspension-history" type="button" role="tab" aria-controls="sb-suspension-history" aria-selected="false" tabindex="-1">
              <span>
                Suspensions
              </span>
            </button>
          </div>
        </div>

        <div class="tab-content mt-15" id="v-pills-tabContent">
          <!-- Account Info -->
          <div class="tab-pane fade  show active" id="account-info" role="tabpanel" aria-labelledby="account-info-tab">
            <div class="user-data-show">
              <div class="user-info-part mb-3">
                <h5>Personal Information</h5>
                <div class="row gy-lg-4 gy-3">
                  <div class="col-lg-3">
                    <label for="">Full Name</label>
                    <span>
                      <%= body?.user?.userName %>
                    </span>
                  </div>
                  <div class="col-lg-3">
                    <label for="">Email Address</label>
                    <span>
                      <%= body?.user?.email %>
                      <svg class="ms-1" width="15" height="16" viewBox="0 0 15 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <circle cx="7" cy="8" r="5" fill="white"></circle>
                        <path d="M14.4523 6.66766L13.1079 5.32141V4.24766C13.1076 3.75174 12.9106 3.27619 12.5602 2.92529C12.2097 2.57439 11.7344 2.37677 11.2385 2.37578H10.166L8.82165 1.03016C8.11477 0.323281 6.88352 0.323281 6.17727 1.03016L4.8329 2.37578H3.7604C3.26443 2.37661 2.78905 2.57417 2.43858 2.92511C2.08811 3.27604 1.89119 3.75169 1.89102 4.24766V5.32141L0.546021 6.66766C0.196305 7.01933 0 7.49513 0 7.99109C0 8.48706 0.196305 8.96286 0.546021 9.31453L1.8904 10.6608V11.7345C1.89073 12.2305 2.0877 12.706 2.43814 13.0569C2.78857 13.4078 3.26385 13.6054 3.75977 13.6064H4.83227L6.17665 14.952C6.52977 15.3052 6.99977 15.5002 7.49915 15.5002C7.99852 15.5002 8.4679 15.3052 8.82102 14.952L10.1654 13.6064H11.2379C11.7339 13.6056 12.2092 13.408 12.5597 13.0571C12.9102 12.7061 13.1071 12.2305 13.1073 11.7345V10.6608L14.4523 9.31453C14.802 8.96286 14.9983 8.48706 14.9983 7.99109C14.9983 7.49513 14.802 7.01933 14.4523 6.66766ZM11.376 6.88203L8.21727 10.0039C7.83352 10.3852 7.3279 10.5764 6.8229 10.5764C6.3179 10.5764 5.81477 10.3858 5.42977 10.0058L3.8054 8.44391C3.74708 8.38613 3.70072 8.31742 3.66895 8.24172C3.63719 8.16602 3.62065 8.08481 3.62027 8.00272C3.61989 7.92062 3.63569 7.83926 3.66675 7.76327C3.69782 7.68728 3.74355 7.61816 3.80133 7.55984C3.85911 7.50153 3.92782 7.45517 4.00352 7.4234C4.07922 7.39164 4.16043 7.37509 4.24252 7.37472C4.32462 7.37434 4.40598 7.39014 4.48197 7.4212C4.55796 7.45227 4.62708 7.498 4.6854 7.55578L6.30915 9.11703C6.446 9.25215 6.63058 9.32791 6.8229 9.32791C7.01521 9.32791 7.19979 9.25215 7.33665 9.11703L10.496 5.99453C10.6138 5.87784 10.7731 5.81271 10.9389 5.81347C11.1047 5.81423 11.2634 5.88082 11.3801 5.99859C11.4968 6.11637 11.5619 6.27567 11.5611 6.44147C11.5604 6.60726 11.4938 6.76534 11.376 6.88203Z" fill="#00994C"></path>
                      </svg>
                    </span>
                  </div>
                  <div class="col-lg-3">
                    <label for="">Country</label>
                    <span>
                      <%= body?.user?.country || "---" %>
                    </span>
                  </div>
                  <div class="col-lg-3">
                    <label for="">Age</label>
                    <span>
                      <%= body?.user?.age || "---" %>
                    </span>
                  </div>
                  <div class="col-lg-3">
                    <label for="">Mailing Address</label>
                    <span>
                      <%= body?.user?.address || "---" %>
                    </span>
                  </div>
                </div>
              </div>
              <div class="user-info-part">
                <h5>Activity Summary</h5>
                <div class="row gy-lg-4 gy-3">
                  <div class="col-lg-3">
                    <label for="">Total Entries</label>
                    <span class="fs-md-3 fs-4 fw-semibold">0</span>
                    <!-- <span class="fs-md-3 fs-4 fw-semibold"><%- body?.totalEntries %></span> -->
                  </div>
                  <div class="col-lg-3">
                    <label for="">Total Spent (USD) </label>
                    <!-- <span class="fs-md-3 fs-4 fw-semibold">$<%- body?.totalSpent %></span> -->
                    <span class="fs-md-3 fs-4 fw-semibold">$0</span>
                  </div>
                  <div class="col-lg-3">
                    <label for="">Member Since</label>
                    <span class="fs-md-3 fs-4 fw-semibold">
                      <%= new Date(body?.user.created_at).toLocaleDateString('en-IN') %>
                    </span>
                  </div>
                  <div class="col-lg-3">
                    <label for="">Last Login</label>
                    <span class="fs-md-3 fs-4 fw-semibold">
                      <%= new Date(body?.lastLoginDate).toLocaleDateString('en-IN') %>
                    </span>
                  </div>

                </div>
              </div>
            </div>
          </div>
          <!-- Product Limits -->
          <div class="tab-pane fade" id="celeb-limit" role="tabpanel" aria-labelledby="celeb-limit-tab">
            <div class="pb-3">
              <div class="row gy-4">
                <div class="col-xl-4 col-md-6">
                  <div class="static-card bg-white">
                    <div class="d-flex justify-content-between align-items-center">
                      <div class="static-data">
                        <span class="sd-title fs-14 d-block">Total Spent on
                          Celebrities</span>
                        <!-- <span class="sd-data">$<%= body?.totalSpent || 0 %></span> -->
                        <span class="sd-data">$0</span>
                      </div>
                      <div class="sc-img">
                        <img src="/images/dollor.svg" alt="bot">
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-xl-4 col-md-6">
                  <div class="static-card bg-white">
                    <div class="d-flex justify-content-between align-items-center">
                      <div class="static-data">
                        <span class="sd-title fs-14 d-block">Total Paid Plans</span>
                        <!-- <span class="sd-data"><%= body?.totalPaidPlan %></span> -->
                        <span class="sd-data">0</span>
                      </div>
                      <div class="sc-img">
                        <img src="/images/redeem.svg" alt="bot">
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-xl-4 col-md-6">
                  <div class="static-card bg-white">
                    <div class="d-flex justify-content-between align-items-center">
                      <div class="static-data">
                        <span class="sd-title fs-14 d-block">Total Entries</span>
                        <!-- <span class="sd-data"><%- body?.totalEntries %></span> -->
                        <span class="sd-data">0</span>
                      </div>
                      <div class="sc-img">
                        <img src="/images/user.svg" alt="bot">
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
